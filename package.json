{
  "name": "portfolio",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "0.5.10",
    "astro": "4.7.1",
    "typescript": "5.4.5"
  },
  "engines": {
    "npm": "9.0.6",
    "node": "20.12.2"
  },
  "packageManager": "pnpm@9.0.6",
  "devDependencies": {
    "@types/node": "20.14.2",
    "prettier": "3.3.2"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
